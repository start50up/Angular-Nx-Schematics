    "prepare.ionic.start50up-mobile": "npm run clean && npm run clean.ionic.start50up-mobile && npm run build.ionic.start50up-mobile",
    "prepare.ionic.start50up-mobile.ios": "npm run prepare.ionic.start50up-mobile && cd apps/ionic-start50up-mobile && npm run cap.add.ios",
    "prepare.ionic.start50up-mobile.android": "npm run prepare.ionic.start50up-mobile && cd apps/ionic-start50up-mobile && npm run cap.add.android",
    "open.ionic.start50up-mobile.ios": "cd apps/ionic-start50up-mobile && npm run cap.ios",
    "open.ionic.start50up-mobile.android": "cd apps/ionic-start50up-mobile && npm run cap.android",
    "sync.ionic.start50up-mobile": "cd apps/ionic-start50up-mobile && npm run cap.copy",
    "start.ionic.start50up-mobile": "cd apps/ionic-start50up-mobile && npm start",
    "clean.ionic.start50up-mobile": "cd apps/ionic-start50up-mobile && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json"
  },
